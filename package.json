{
  "name": "taro-app",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "cleveng",
    "email": "cleveng@gmail.com"
  },
  "appid": "xxxx",
  "license": "MIT",
  "description": "vemdom",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "export NODE_OPTIONS=--openssl-legacy-provider && bun run build:weapp -- --watch --no-cache",
    "dev:swan": "bun run build:swan -- --watch",
    "dev:alipay": "bun run build:alipay -- --watch",
    "dev:tt": "bun run build:tt -- --watch",
    "dev:h5": "export NODE_OPTIONS=--openssl-legacy-provider && bun run build:h5 -- --watch",
    "dev:rn": "bun run build:rn -- --watch",
    "dev:qq": "bun run build:qq -- --watch",
    "dev:jd": "bun run build:jd -- --watch",
    "dev:harmony-hybrid": "bun run build:harmony-hybrid -- --watch",
    "lint": "prettier --write . && stylelint --allow-empty-input \"src/*.{css,scss}\"",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "postinstall": "weapp-tw patch",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "lint-staged": {
    "src/*.{js,ts,tsx}": "eslint --fix",
    "src/*.{js,ts,css,scss}": "prettier --write --ignore-unknown"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/runtime": "^7.25.7",
    "@cakioe/kit.js": "^1.6.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/introspection": "^4.0.3",
    "@nutui/icons-react-taro": "^1.0.5",
    "@nutui/nutui-react-taro": "^2.6.21",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@redux-devtools/extension": "^3.3.0",
    "@tarojs/components": "4.0.5",
    "@tarojs/helper": "4.0.5",
    "@tarojs/plugin-framework-react": "4.0.5",
    "@tarojs/plugin-html": "4.0.6",
    "@tarojs/plugin-platform-alipay": "4.0.5",
    "@tarojs/plugin-platform-h5": "4.0.5",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.5",
    "@tarojs/plugin-platform-jd": "4.0.5",
    "@tarojs/plugin-platform-qq": "4.0.5",
    "@tarojs/plugin-platform-swan": "4.0.5",
    "@tarojs/plugin-platform-tt": "4.0.5",
    "@tarojs/plugin-platform-weapp": "4.0.5",
    "@tarojs/react": "4.0.5",
    "@tarojs/runtime": "4.0.5",
    "@tarojs/shared": "4.0.5",
    "@tarojs/taro": "4.0.5",
    "@tarojs/webpack5-runner": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-import": "^1.13.8",
    "cache-loader": "^4.1.0",
    "commitlint": "^19.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "immer": "^10.1.1",
    "lint-staged": "^15.2.10",
    "postcss-rem-to-responsive-pixel": "^6.0.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^5.3.0",
    "simple-zustand-devtools": "^1.1.0",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.4.13",
    "taro-plugin-compiler-optimization": "^1.0.4",
