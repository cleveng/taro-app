    "@tarojs/cli": "4.0.5",
    "@types/react": "^18.3.11",
    "babel-preset-taro": "4.0.6",
    "eslint": "8.57.1",
    "eslint-config-taro": "4.0.6",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.47",
    "react-refresh": "^0.14.2",
    "sass": "^1.79.4",
    "terser": "^5.34.1",
    "typescript": "^5.6.2"
  }
}
