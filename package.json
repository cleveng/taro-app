{
  "name": "vemdom",
  "version": "1.0.0",
  "private": true,
  "description": "vemdom",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "bun run build:weapp -- --watch",
    "dev:swan": "bun run build:swan -- --watch",
    "dev:alipay": "bun run build:alipay -- --watch",
    "dev:tt": "bun run build:tt -- --watch",
    "dev:h5": "bun run build:h5 -- --watch",
    "dev:rn": "bun run build:rn -- --watch",
    "dev:qq": "bun run build:qq -- --watch",
    "dev:jd": "bun run build:jd -- --watch",
    "dev:harmony-hybrid": "bun run build:harmony-hybrid -- --watch",
    "lint": "prettier --write . && stylelint --allow-empty-input \"src/*.{css,scss}\"",
    "prettier": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/*.{js,ts,tsx}": "eslint --fix",
    "src/*.{js,ts,css,scss}": "prettier --write --ignore-unknown"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@tarojs/components": "4.0.5",
    "@tarojs/helper": "4.0.5",
    "@tarojs/plugin-platform-weapp": "4.0.5",
    "@tarojs/plugin-platform-alipay": "4.0.5",
    "@tarojs/plugin-platform-tt": "4.0.5",
    "@tarojs/plugin-platform-swan": "4.0.5",
    "@tarojs/plugin-platform-jd": "4.0.5",
    "@tarojs/plugin-platform-qq": "4.0.5",
    "@tarojs/plugin-platform-h5": "4.0.5",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.5",
    "@tarojs/runtime": "4.0.5",
    "@tarojs/shared": "4.0.5",
    "@tarojs/taro": "4.0.5",
    "@tarojs/plugin-framework-react": "4.0.5",
    "@tarojs/react": "4.0.5",
    "react-dom": "^18.0.0",
    "react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "autoprefixer": "^10.4.20",
    "commitlint": "^19.4.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@tarojs/cli": "4.0.5",
    "@tarojs/vite-runner": "4.0.5",
    "babel-preset-taro": "4.0.5",
    "eslint-config-taro": "4.0.5",
    "eslint": "^8.57.0",
    "stylelint": "^16.4.0",
    "terser": "^5.30.4",
    "vite": "^4.2.0",
    "@babel/preset-react": "^7.24.1",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "react-refresh": "^0.14.0",
    "@preact/preset-vite": "^2.8.2",
    "@prefresh/core": "^1.5.2",
    "@prefresh/babel-plugin": "^0.5.1",
    "sass": "^1.75.0",
    "typescript": "^5.4.5",
    "postcss": "^8.4.38"
  }
}
